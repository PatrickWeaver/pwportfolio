extends layout

block content
	a(href="/projects/") â¬… Projects

	h1= title

	ul
			li= "Status: " + project.status
			li= "Start Date: " + project.startDate
			li= "End Date: " + project.endDate
			li= "Tags: " + project.tags
			li= "Project URL: " + project.projectURL
			li= "Source URL: " + project.sourceURL
			li
				ul
					each image, index in project.images
						img(src=image.url)

	a(href="/projects/" + project.slug + "/upload") Upload